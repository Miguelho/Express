extends layout

block content
  div(ng-controller='HomeController')
    p Visible solo para usuarios loggeados!
    p {{message}}

    table(class="tablaUsuarios")
     tr
      th Id
      th Nombre
      tr(ng-repeat="user in users")
       td {{user._id}}
       td {{user.username}}
       td#botonera_tablaUsuarios(ng-click="delete(user)") Borrar
       td#botonera_tablaUsuarios Editar
  


